{
  "name": "Google Dinosaur Game Modified Version",
  "version": "1.0",
  "description": " modified game",
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": [
      "background.js",
       "css/adminlte.min.css",
      "scripts/bootstrap.bundle.min.js",
      "scripts/firRunner.js",
      "scripts/jquery.js",
      "scripts/playerNum.js",
      "scripts/runner.js",
      "scripts/secRunner.js",
      "scripts/selector.js",
      "scripts/twoRunner.js",

      "gaming.html",
      "index.css",
      "index.html"


    ]
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],

  "web_accessible_resources": [
    "css/adminlte.min.css",
    "scripts/bootstrap.bundle.min.js",
    "scripts/firRunner.js",
    "scripts/jquery.js",
    "scripts/playerNum.js",
    "scripts/runner.js",
    "scripts/secRunner.js",
    "scripts/selector.js",
    "scripts/twoRunner.js",

    "gaming.html",
    "index.css",
    "index.html"


  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "game"
  }

}
